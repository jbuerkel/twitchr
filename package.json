{
  "name": "twitchr",
  "version": "0.0.2",
  "description": "A twitch bot providing IRC based assistance",
  "author": "Jonas BÃ¼rkel <buerkelj@gmail.com>",
  "scripts": {
    "dev": "gulp dev",
    "dist": "npm run dist.client && npm run dist.plugins && npm run dist.server",
    "dist.client": "rimraf ./dist/client/ && gulp dist.client",
    "dist.plugins": "rimraf ./dist/plugins/ && gulp dist.plugins",
    "dist.server": "rimraf ./dist/server/ && gulp dist.server",
    "gulp": "gulp",
    "http": "node ./dist/server/bin/http.js",
    "https": "node ./dist/server/bin/https.js",
    "lint": "npm run lint.client && npm run lint.server",
    "lint.client": "gulp lint.client",
    "lint.server": "gulp lint.server",
    "postinstall": "npm prune && npm run typings.install && npm run dist",
    "start": "npm run https",
    "typings": "typings",
    "typings.install": "rimraf ./typings/ && typings install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/twitchr/twitchr"
  },
  "bugs": {
    "url": "https://github.com/twitchr/twitchr/issues"
  },
  "keywords": [
    "twitch",
    "bot",
    "irc"
  ],
  "license": "GPL-3.0",
  "dependencies": {
    "angular2": "^2.0.0-beta.17",
    "app-root-path": "^1.0.0",
    "bootstrap": "^3.3.6",
    "connect-mongo": "^1.2.0",
    "debug": "^2.2.0",
    "dotenv-safe": "^2.3.1",
    "es6-shim": "^0.35.0",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "glob": "^7.0.3",
    "helmet": "^2.1.0",
    "irc": "^0.5.0",
    "morgan": "^1.7.0",
    "reflect-metadata": "^0.1.3",
    "request": "^2.72.0",
    "rxjs": "5.0.0-beta.6",
    "serve-favicon": "^2.3.0",
    "systemjs": "^0.19.29",
    "twitchr-plugin-api": "^0.1.0",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "browser-sync": "^2.12.11",
    "cssnano": "^3.6.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
